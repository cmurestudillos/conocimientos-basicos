{
  "index": "/index.html",
  "assetGroups": [
    {
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  },
  {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/images/pokemon.png",
        "/assets/images/pokeball.png"
      ]
    }
  }
],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api/pokemon.json"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 15,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}